<?xml version='1.0' encoding='utf-8'?>
<widget id="com.test.test1" version="1.0.0" android-versionCode="8.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>HelloCordova</name>
  <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author>
    Tadapp
  </author>
  <content src="index.html" />
  <plugin name='cordova-plugin-splashscreen' />
  <plugin name='cordova-plugin-inappbrowser' />
  <plugin name='cordova-plugin-network-information' />
  <preference name='phonegap-version' value='cli-9.0.0' />
  <gap:plugin name='onesignal-cordova-plugin' spec='2.4.5' source='npm' />
  <preference name='android-build-tool' value='gradle' />
  <preference name="android-targetSdkVersion" value="28" />
  <preference name='orientation' value='portrait' />
  <preference name="SplashMaintainAspectRatio" value="false" />
  <preference name="SplashScreenSpinnerColor" value="#ffffff" />
  <preference name="Fullscreen" value="false" />

  <platform name="android">
    <icon background="res/icon/android/ldpi_1616280047_62bcce5bc1d204c540dd.png" density="ldpi" foreground="res/icon/android/ldpi_1616280047_62bcce5bc1d204c540dd.png" />
    <icon background="res/icon/android/mdpi_1616280047_62bcce5bc1d204c540dd.png" density="mdpi" foreground="res/icon/android/mdpi_1616280047_62bcce5bc1d204c540dd.png" />
    <icon background="res/icon/android/hdpi_1616280047_62bcce5bc1d204c540dd.png" density="hdpi" foreground="res/icon/android/hdpi_1616280047_62bcce5bc1d204c540dd.png" />
    <icon background="res/icon/android/xhdpi_1616280047_62bcce5bc1d204c540dd.png" density="xhdpi" foreground="res/icon/android/xhdpi_1616280047_62bcce5bc1d204c540dd.png" />
    <icon background="res/icon/android/xxhdpi_1616280047_62bcce5bc1d204c540dd.png" density="xxhdpi" foreground="res/icon/android/xxhdpi_1616280047_62bcce5bc1d204c540dd.png" />
    <icon background="res/icon/android/xxxhdpi_1616280047_62bcce5bc1d204c540dd.png" density="xxxhdpi" foreground="res/icon/android/xxxhdpi_1616280047_62bcce5bc1d204c540dd.png" />
        
    <splash src="res/screen/android/hdpi_port_1616280181_5d98825c56487c784667.png" density="port-hdpi" />
    <splash src="res/screen/android/ldpi_port_1616280181_5d98825c56487c784667.png" density="port-ldpi" />
    <splash src="res/screen/android/mdpi_port_1616280181_5d98825c56487c784667.png" density="port-mdpi" />
    <splash src="res/screen/android/xhdpi_port_1616280181_5d98825c56487c784667.png" density="port-xhdpi" />
    <splash src="res/screen/android/hdpi_land_1616280181_5d98825c56487c784667.png" density="land-hdpi" />
    <splash src="res/screen/android/ldpi_land_1616280181_5d98825c56487c784667.png" density="land-ldpi" />
    <splash src="res/screen/android/mdpi_land_1616280181_5d98825c56487c784667.png" density="land-mdpi" />
    <splash src="res/screen/android/xhdpi_land_1616280181_5d98825c56487c784667.png" density="land-xhdpi" />
  </platform>
  <access origin='*'/>
  <plugin name='cordova-plugin-whitelist' />
  <allow-intent href='http://*/*'/>
  <allow-intent href='https://*/*'/>
  <allow-intent href='tel:*'/>
  <allow-intent href='sms:*'/>
  <allow-intent href='mailto:*'/>
  <allow-intent href='geo:*'/>
</widget>
